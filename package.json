  "build": {
    "productName": "mpvue-html-electron-typescript-starter",
    "appId": "",
    "directories": {
      "output": "output-electron-temp"
    },
    "files": [
      "dist-electron/electron/**/*"
    ],
    "dmg": {
      "contents": [{
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build-electron/icons/icon.icns"
    },
    "win": {
      "icon": "build-electron/icons/icon.ico"
    },
    "linux": {
      "icon": "build-electron/icons"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
